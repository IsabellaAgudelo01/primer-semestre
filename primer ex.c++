#include <iostream>
using namespace std;
double a;
double b;
int main(){
    cout << "Agrega un número";
    cin >> a;

    cout << "Agrega otro número";
    cin >> b;

    cout << "La suma es " << a+b;
    cout << "La resta es " << a-b;
    cout << "La multiplicación es " << a*b;
    cout << "La división es " << a/b;


return 0;
}
